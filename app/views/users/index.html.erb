<h1>Users</h1>
<%= paginate @users %>
<table class='table table-condensed table-hover'>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>City</th>
      <th>State</th>
